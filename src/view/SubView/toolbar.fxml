<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="#mainPaneId" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="700.0" minWidth="910.0" prefHeight="700.0" prefWidth="910.0"
	stylesheets="@../styleSheetPaint.css" xmlns="http://javafx.com/javafx/8.0.60"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SubController.ToolBarCtrl">
	<children>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<items>
				<Button id="SelectionTool" maxHeight="33.0" maxWidth="40.0" minHeight="33.0"
					minWidth="40.0" mnemonicParsing="false" prefHeight="33.0"
					prefWidth="40.0" />
				<Button id="PencilTool" layoutX="10.0" layoutY="10.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" onMouseClicked="#pencilSketching"/>
				<Button id="BrushTool" layoutX="55.0" layoutY="10.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" onMouseClicked="#burshSketching"/>
				<Button id="FillingTool" layoutX="100.0" layoutY="10.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" onMouseClicked="#fillingbackground"/>
				<Button id="EraseTool" layoutX="145.0" layoutY="10.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" onMouseClicked="#eraserTool"/>
				<Button id="TextTool" layoutX="190.0" layoutY="10.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
				<ColorPicker fx:id="colorPickerFXid"/>
				<Slider fx:id="sliderFXid" blockIncrement="5.0" majorTickUnit="5.0" max="30.0"
					min="5.0" minorTickCount="5" showTickLabels="true" />
				<Button layoutX="235.0" layoutY="14.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
				<Button layoutX="235.0" layoutY="14.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
				<Button layoutX="235.0" layoutY="14.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
				<Button layoutX="235.0" layoutY="14.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
				<Button layoutX="235.0" layoutY="14.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
				<Button layoutX="235.0" layoutY="14.0" maxHeight="33.0"
					maxWidth="40.0" minHeight="33.0" minWidth="40.0" mnemonicParsing="false"
					prefHeight="33.0" prefWidth="40.0" />
			</items>
		</ToolBar>
	</children>
</AnchorPane>
